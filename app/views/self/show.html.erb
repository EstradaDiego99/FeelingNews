# <h1>Self#show</h1>
# <p>Find me in app/views/self/show.html.erb</p>


<% @posts.each do |post| %>

    <section id="showSection">   
        <div class="row">
            <%= post.texto %>
        </div>  
        <div class="row">
            <%= post.favs %>
        </div>
        <div class="row">
            <%= post.hashtags %>
        </div>
        <div class="row">
            <%= post.shares %>
        </div>
        <div class="row">
            <%= post.tags %>
        </div>
        <div class="row">
            <%= post.concept %>
        </div>
        <div class="row">
            <%= post.concept_score %>
        </div>
        <div class="row">
            <%= post.keyword %>
        </div>
        <div class="row">
            <%= post.keyword_score %>
        </div>
        <div class="row">
            <%= post.category %>
        </div>     
        <div class="row">
            <%= post.category_score %>
        </div>                           
        <div class="row">
            <%= post.entity %>
        </div>
        <div class="row">
            <%= post.entity_score %>
        </div>
        <div class="row">
            <%= link_to 'Show', post %>
        </div>                            
    </section>

  <% end %>